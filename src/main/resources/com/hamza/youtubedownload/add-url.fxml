<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <HBox spacing="5.0">
               <children>
                  <Label fx:id="welcomeText" text="Url" />
                  <TextField fx:id="text_url" maxWidth="1.7976931348623157E308" prefWidth="350.0" HBox.hgrow="SOMETIMES" />
                  <VBox spacing="5.0">
                     <children>
                        <Button fx:id="search" text="Search" />
                        <Button fx:id="search1" layoutX="10.0" layoutY="10.0" text="Cancel" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
